{
  "presets": [
    [
      "@babel/preset-env",
      {
        "corejs": { "version": "3.22" },
        "useBuiltIns": "usage",
        "targets": {
          "ie": "11"
        }
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    "@babel/plugin-syntax-dynamic-import",
    "@babel/plugin-transform-async-to-generator",
    "@babel/plugin-transform-modules-commonjs"
  ]
}